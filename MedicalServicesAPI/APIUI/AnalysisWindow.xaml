<Window x:Class="APIUI.AnalysisWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:APIUI"
		mc:Ignorable="d"
		WindowStartupLocation="CenterScreen"
		Title="AnalysisWindow"
		Height="450" Width="800">
	<Grid>
		<TextBox Margin="0,7.5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="25" />
		<DataGrid AutoGenerateColumns="False" Margin="0,40,0,0">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Название анализа" Binding="{Binding Name}" />
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</Window>